# Audio Analysis API Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# OpenAI API (for AI-powered analysis)
OPENAI_API_KEY=your_openai_api_key_here

# Application Configuration
PRONUNCIATION_ANALYSIS_APP_ID=pronunciation_analysis_app_v1
PRONUNCIATION_ANALYSIS_PRIMARY_API_KEY=your_primary_api_key_here
PRONUNCIATION_ANALYSIS_SECONDARY_API_KEY=your_secondary_api_key_here

# Development Settings
DEBUG=true
LOG_LEVEL=INFO 